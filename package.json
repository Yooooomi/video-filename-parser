{
  "name": "@ctrl/video-filename-parser",
  "version": "0.0.0-placeholder",
  "description": "A radarr style release name parser",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/video-filename-parser",
  "homepage": "https://video-filename-parser.vercel.app",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "scene",
    "release",
    "radarr",
    "sonarr",
    "typescript"
  ],
  "scripts": {
    "demo:watch": "snowpack dev",
    "demo:build": "snowpack build",
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "7.14.5",
    "@babel/preset-typescript": "7.14.5",
    "@ctrl/eslint-config": "2.0.8",
    "@fullhuman/postcss-purgecss": "4.0.3",
    "@jest/globals": "27.0.6",
    "@snowpack/plugin-postcss": "1.4.3",
    "@tailwindcss/forms": "0.3.3",
    "@types/node": "15.12.5",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.8",
    "autoprefixer": "10.2.6",
    "cssnano": "5.0.6",
    "jest": "27.0.6",
    "jest-junit": "12.2.0",
    "postcss": "8.3.5",
    "postcss-cli": "8.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "snowpack": "3.6.2",
    "tailwindcss": "2.2.4",
    "typescript": "4.3.4"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=12"
  }
}
