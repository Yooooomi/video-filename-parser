{
  "name": "@ctrl/video-filename-parser",
  "version": "0.0.0-placeholder",
  "description": "A radarr style release name parser",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/video-filename-parser",
  "homepage": "https://video-filename-parser.vercel.app",
  "type": "module",
  "exports": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "sideEffects": false,
  "keywords": [
    "scene",
    "release",
    "radarr",
    "sonarr"
  ],
  "scripts": {
    "demo:build": "npm run build --workspace=demo",
    "demo:watch": "npm run dev --workspace=demo",
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "tsc",
    "test": "ava",
    "test:watch": "ava --watch",
    "test:ci": "c8 --reporter=text --reporter=lcov ava"
  },
  "devDependencies": {
    "@ctrl/eslint-config": "3.1.2",
    "@sindresorhus/tsconfig": "2.0.0",
    "@types/node": "16.11.10",
    "ava": "4.0.0-rc.1",
    "c8": "7.10.0",
    "expect": "27.3.1",
    "tsm": "2.1.4",
    "typescript": "4.5.2"
  },
  "ava": {
    "files": [
      "test/**/*.spec.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=tsm"
    ]
  },
  "workspaces": [
    "demo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=14"
  }
}
